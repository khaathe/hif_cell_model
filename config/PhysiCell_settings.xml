<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-1000</x_min>
		<x_max>1000</x_max>
		<y_min>-1000</y_min>
		<y_max>1000</y_max>
		<z_min>-10</z_min>
		<z_max>10</z_max>
		<dx>20</dx>
		<dy>20</dy>
		<dz>20</dz>
		<use_2D>true</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">7200</max_time> <!-- 5 days * 24 h * 60 min -->
		<time_units>min</time_units>
		<space_units>micron</space_units>
	
		<dt_diffusion units="min">0.01</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">6</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>10</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>output</folder> <!-- use . for root --> 

		<full_data>
			<interval units="min">1440</interval>
			<enable>true</enable>
		</full_data>
		
		<SVG>
			<interval units="min">720</interval>
			<enable>true</enable>
		</SVG>
		
		<legacy_data>
			<enable>false</enable>
		</legacy_data>
	</save>
	
	<options>
		<legacy_random_points_on_sphere_in_divide>false</legacy_random_points_on_sphere_in_divide>
	</options>	

	<microenvironment_setup>
		<variable name="oxygen" units="mmHg" ID="0">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">100000.0</diffusion_coefficient>
				<decay_rate units="1/min">0.1</decay_rate>  
			</physical_parameter_set>
			<initial_condition units="mmHg">160.0</initial_condition> <!-- Physicell Default : 38.0 mmHg - 5% -->
			<Dirichlet_boundary_condition units="mmHg" enabled="true">160.0</Dirichlet_boundary_condition> <!-- Physicell Default : 38.0 mmHg - 5% -->
		</variable>
		
		<options>
			<calculate_gradients>true</calculate_gradients>
			<track_internalized_substrates_in_each_agent>true</track_internalized_substrates_in_each_agent>
			<!-- not yet supported --> 
			<initial_condition type="matlab" enabled="false">
				<filename>./config/initial.mat</filename>
			</initial_condition>
			<!-- not yet supported --> 
			<dirichlet_nodes type="matlab" enabled="false">
				<filename>./config/dirichlet.mat</filename>
			</dirichlet_nodes>
		</options>
	</microenvironment_setup>	
	
	<user_parameters>
		<random_seed type="int" units="dimensionless">0</random_seed> 
		<!-- example parameters from the template --> 
		
		<!-- motile cell type parameters --> 
		<motile_cell_persistence_time type="double" units="min">15</motile_cell_persistence_time>
		<motile_cell_migration_speed type="double" units="micron/min">0.5</motile_cell_migration_speed> 
		<motile_cell_relative_adhesion type="double" units="dimensionless">0.05</motile_cell_relative_adhesion>
		<motile_cell_apoptosis_rate type="double" units="1/min">0.0</motile_cell_apoptosis_rate> 
		<motile_cell_relative_cycle_entry_rate type="double" units="dimensionless">0.1</motile_cell_relative_cycle_entry_rate>
		<tissue_radius type="double" units="micrometers">80.0</tissue_radius> <!-- cell radius = 8 -->
		<o2_hypoxic_threshold type="double" units="mmHg">45.0</o2_hypoxic_threshold> <!-- Physicell Default : 15.0 mmHg - 2% -->
		<o2_hypoxic_response type="double" units="mmHg">45.0</o2_hypoxic_response> <!-- Physicell Default : 8.0 mmHg - 1% -->
		<o2_hypoxic_saturation type="double" units="mmHg">3.0</o2_hypoxic_saturation> <!-- Physicell Default : 4.0 mmHg - 0.5% -->
		<hif_base_concentration type="double" units="dimensionless">0.0085</hif_base_concentration>
	</user_parameters>
	
</PhysiCell_settings>
